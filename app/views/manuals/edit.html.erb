<h1>EDIT Manual Scenario</h1>
<% form_for(@manual) do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :scenario %><br />
    <%= f.text_area :scenario, :size => "100x5"  %>
  </p>
  <p>
    <%= f.label :Pass? %>
    <%= f.check_box :status %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>