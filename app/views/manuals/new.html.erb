<h1>ADD Manual Scenario</h1>
<% form_for(@manual) do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :scenario %><br />
    <%= f.text_area :scenario, :size => "100x5" %>
  </p>
    <p>
      <%= f.label :tag %><br />
      <%= f.select(:tag, Manual::TAGS, {:include_blank => 'None'}) %>
    </p>
  <p>
  <%= f.submit 'Create' %>
<% end %>
  <%= link_to "Cancel", manuals_path %>
  </p>