<h1>Editing Result</h1>

<% form_for(@result) do |f| %>
  <%= f.error_messages %>

  <p align="center">
    <%= f.label :false_positive %><br />
    <%= f.check_box :check %>
  </p>
  <p>
    <%= f.label :created_at %><br />
    <%= f.text_field :created_at %>
  </p>
  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :comment %><br />
    <%= f.text_field :comment %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Back', results_path %>